TEST_TARGET=&quot;${SUBPROJ}${test}&quot;
END_TARGET=&quot;${SUBPROJ}${end_target}&quot;

echo &quot;SUBPROJ=${SUBPROJ}&quot; &gt; &quot;${WORKSPACE}/newEnvVar.txt&quot; 
echo &quot;RAWSUB=${RAWSUB}&quot; &gt;&gt; &quot;${WORKSPACE}/newEnvVar.txt&quot; 
