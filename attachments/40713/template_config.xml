      <presendScript>import java.io.File;
File getReplacementIndicatorFile(String subproject) {
     if (subproject == null) { 
          return build.getWorkspace().child(&apos;build&apos;).child(&apos;replaceInfo.txt&apos;)
     } else {
          return build.getWorkspace().child(subproject).child(&apos;build&apos;).child(&apos;replaceInfo.txt&apos;)
     }
}
if (this.hasProperty(&quot;RAWSUB&quot;)) {
    File replaceInfoFile = getReplacementIndicatorFile(&quot;${RAWSUB}&quot;)  
    logger.println(&quot;replaceInfoFile=${replaceInfoFile}&quot;)
    if (replaceInfoFile.exists()) {
        msg.text = replaceInfoFile.readToString()
    } else {
        cancel = true
    }
} else {
   cancel = true
}</presendScript>
