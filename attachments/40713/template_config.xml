      <command>if [ ${end_target} == &quot;deployToSWDepot&quot; ] || [ ${end_target} == &quot;dTSWD&quot; ]
then
	BLANK=&quot;ROOT&quot;
else
	BLANK=
fi

if [ &quot;$BLANK&quot; != &quot;${subProject}&quot; ]
then 
   SUBPROJ=`echo &quot;:${subProject}:&quot; | /bin/sed &apos;s|/|:|g&apos;`
   RAWSUB=&quot;${subProject}&quot;
else
   SUBPROJ=
