<?xml version='1.0' encoding='UTF-8'?>
<project>
  <actions/>
  <description>See &lt;a href=&quot;https://confluence.cgm.ag/display/examiner/Build+pipeline&quot;&gt;build pipeline&lt;/a&gt;</description>
  <keepDependencies>false</keepDependencies>
  <properties>
    <hudson.plugins.buildblocker.BuildBlockerProperty plugin="build-blocker-plugin@1.7.3">
      <useBuildBlocker>false</useBuildBlocker>
      <blockLevel>GLOBAL</blockLevel>
      <scanQueueFor>DISABLED</scanQueueFor>
      <blockingJobs></blockingJobs>
    </hudson.plugins.buildblocker.BuildBlockerProperty>
    <com.chikli.hudson.plugin.naginator.NaginatorOptOutProperty plugin="naginator@1.17.2">
      <optOut>false</optOut>
    </com.chikli.hudson.plugin.naginator.NaginatorOptOutProperty>
    <jenkins.model.BuildDiscarderProperty>
      <strategy class="hudson.tasks.LogRotator">
        <daysToKeep>30</daysToKeep>
        <numToKeep>60</numToKeep>
        <artifactDaysToKeep>5</artifactDaysToKeep>
        <artifactNumToKeep>10</artifactNumToKeep>
      </strategy>
    </jenkins.model.BuildDiscarderProperty>
    <hudson.plugins.disk__usage.DiskUsageProperty plugin="disk-usage@0.28"/>
  </properties>
  <scm class="hudson.scm.NullSCM"/>
  <canRoam>true</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>true</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>true</blockBuildWhenUpstreamBuilding>
  <jdk>jdk-1.8.0</jdk>
  <triggers/>
  <concurrentBuild>false</concurrentBuild>
  <customWorkspace>ihe_master_cd</customWorkspace>
  <builders>
    <org.jenkinsci.plugins.conditionalbuildstep.ConditionalBuilder plugin="conditional-buildstep@1.3.5">
      <runner class="org.jenkins_ci.plugins.run_condition.BuildStepRunner$Fail" plugin="run-condition@1.0"/>
      <runCondition class="org.jenkins_ci.plugins.run_condition.logic.Or" plugin="run-condition@1.0">
        <conditions>
          <org.jenkins__ci.plugins.run__condition.logic.ConditionContainer>
            <condition class="org.jenkins_ci.plugins.run_condition.core.TimeCondition">
              <earliestHours>20</earliestHours>
              <earliestMinutes>0</earliestMinutes>
              <latestHours>23</latestHours>
              <latestMinutes>59</latestMinutes>
              <useBuildTime>true</useBuildTime>
            </condition>
          </org.jenkins__ci.plugins.run__condition.logic.ConditionContainer>
          <org.jenkins__ci.plugins.run__condition.logic.ConditionContainer>
            <condition class="org.jenkins_ci.plugins.run_condition.core.TimeCondition">
              <earliestHours>0</earliestHours>
              <earliestMinutes>0</earliestMinutes>
              <latestHours>7</latestHours>
              <latestMinutes>0</latestMinutes>
              <useBuildTime>true</useBuildTime>
            </condition>
          </org.jenkins__ci.plugins.run__condition.logic.ConditionContainer>
          <org.jenkins__ci.plugins.run__condition.logic.ConditionContainer>
            <condition class="org.jenkins_ci.plugins.run_condition.core.CauseCondition">
              <buildCause>USER_CAUSE</buildCause>
              <exclusiveCause>false</exclusiveCause>
            </condition>
          </org.jenkins__ci.plugins.run__condition.logic.ConditionContainer>
        </conditions>
      </runCondition>
      <conditionalbuilders>
        <hudson.tasks.Maven>
          <targets>-e site -Dnocompile -Dnopackage</targets>
          <mavenName>maven-3.0.5</mavenName>
          <jvmOptions>-DexcludedGroups=$IHE_EXCLUDED_GROUPS -Djava.io.tmpdir=$WORKSPACE/.tmp -Xms256m -Xmx1536m</jvmOptions>
          <properties>dependency.locations.enabled=false
java.awt.headless=true</properties>
          <usePrivateRepository>true</usePrivateRepository>
          <settings class="jenkins.mvn.DefaultSettingsProvider"/>
          <globalSettings class="jenkins.mvn.DefaultGlobalSettingsProvider"/>
          <injectBuildVariables>true</injectBuildVariables>
        </hudson.tasks.Maven>
        <hudson.tasks.Maven>
          <targets>-e site:deploy -Dnocompile -Dnopackage</targets>
          <mavenName>maven-3.0.5</mavenName>
          <jvmOptions>-DexcludedGroups=$IHE_EXCLUDED_GROUPS -Djava.io.tmpdir=$WORKSPACE/.tmp -Xms256m -Xmx1536m</jvmOptions>
          <properties>java.awt.headless=true</properties>
          <usePrivateRepository>true</usePrivateRepository>
          <settings class="jenkins.mvn.DefaultSettingsProvider"/>
          <globalSettings class="jenkins.mvn.DefaultGlobalSettingsProvider"/>
          <injectBuildVariables>true</injectBuildVariables>
        </hudson.tasks.Maven>
      </conditionalbuilders>
    </org.jenkinsci.plugins.conditionalbuildstep.ConditionalBuilder>
  </builders>
  <publishers/>
  <buildWrappers>
    <org.jenkinsci.plugin.Diskcheck plugin="diskcheck@0.26">
      <failOnError>true</failOnError>
    </org.jenkinsci.plugin.Diskcheck>
  </buildWrappers>
</project>