<?xml version='1.0' encoding='UTF-8'?>
<project>
  <actions/>
  <description>This will run the project gold level regression. If successful the gold label will be updated. This is run weekly.</description>
  <logRotator class="hudson.tasks.LogRotator">
    <daysToKeep>5</daysToKeep>
    <numToKeep>-1</numToKeep>
    <artifactDaysToKeep>-1</artifactDaysToKeep>
    <artifactNumToKeep>-1</artifactNumToKeep>
  </logRotator>
  <keepDependencies>false</keepDependencies>
  <properties/>
  <scm class="org.jenkinsci.plugins.p4.PerforceScm" plugin="p4@1.0.13">
    <credential>XXXXXXXX</credential>
    <workspace class="org.jenkinsci.plugins.p4.workspace.StreamWorkspaceImpl">
      <charset>none</charset>
      <streamName>//project/main</streamName>
      <format>jenkins-${NODE_NAME}-${JOB_NAME}</format>
    </workspace>
    <populate class="org.jenkinsci.plugins.p4.populate.AutoCleanImpl">
      <have>true</have>
      <force>false</force>
      <pin>silver</pin>
      <replace>true</replace>
      <delete>true</delete>
    </populate>
  </scm>
  <assignedNode>yeti</assignedNode>
  <canRoam>false</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers>
    <hudson.triggers.SCMTrigger>
      <spec>H H(0-3) * * 7</spec>
      <ignorePostCommitHooks>false</ignorePostCommitHooks>
    </hudson.triggers.SCMTrigger>
  </triggers>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
      <command>#!/bin/bash

set -e
source project.bashrc

# no gold regression yet so...
/bin/true
</command>
    </hudson.tasks.Shell>
  </builders>
  <publishers>
    <org.jenkinsci.plugins.p4.tagging.TagNotifier plugin="p4@1.0.13">
      <rawLabelName>gold</rawLabelName>
      <rawLabelDesc>Jenkins post-build label.</rawLabelDesc>
      <onlyOnSuccess>true</onlyOnSuccess>
    </org.jenkinsci.plugins.p4.tagging.TagNotifier>
    <hudson.tasks.Mailer plugin="mailer@1.11">
      <recipients>project@company.com</recipients>
      <dontNotifyEveryUnstableBuild>false</dontNotifyEveryUnstableBuild>
      <sendToIndividuals>false</sendToIndividuals>
    </hudson.tasks.Mailer>
  </publishers>
  <buildWrappers/>
</project>
