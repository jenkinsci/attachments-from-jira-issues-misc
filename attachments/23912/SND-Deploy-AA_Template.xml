        <propertiesContent>ARTIFACT_DEPLOYSTATUS=s</propertiesContent>
      </info>
    </EnvInjectBuilder>
    <org.jenkinsci.plugins.conditionalbuildstep.singlestep.SingleConditionalBuilder plugin="conditional-buildstep@1.2.2">
