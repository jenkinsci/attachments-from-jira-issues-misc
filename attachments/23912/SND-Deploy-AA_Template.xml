            <a class="string-array">
              <string>none</string>
            </a>
          </choices>
        </hudson.model.ChoiceParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>ARTIFACT_FULLPATH</name>
          <description>The Fully qualified path of the artifact to deploy.  If this value is empty then the last successful artifact will be deployed.</description>
          <defaultValue></defaultValue>
        </hudson.model.StringParameterDefinition>
        <hudson.model.BooleanParameterDefinition>
          <name>ARM_CALLBACK</name>
          <description>Issue the Call Back to ARM for this Build?</description>
          <defaultValue>true</defaultValue>
