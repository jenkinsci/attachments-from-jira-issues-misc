      <wasServerName>WSB-TEST1</wasServerName>
    </com.michelin.cio.hudson.plugins.wasbuilder.WASBuildStep>
    <hudson.plugins.parameterizedtrigger.TriggerBuilder plugin="parameterized-trigger@2.18">
