    </hudson.plugins.chucknorris.CordellWalkerRecorder>
    <hudson.tasks.BuildTrigger>
      <childProjects>ARMOUR - Release Builder</childProjects>
      <threshold>
        <name>SUCCESS</name>
        <ordinal>0</ordinal>
        <color>BLUE</color>
        <completeBuild>true</completeBuild>
      </threshold>
    </hudson.tasks.BuildTrigger>
    <hudson.plugins.nunit.NUnitPublisher plugin="nunit@0.15">
      <testResultsPattern>build/**/Armour.Tests.Unit.dll-results.xml</testResultsPattern>
